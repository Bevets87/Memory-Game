{
  "name": "memory",
  "version": "1.0.0",
  "description": "the classic memory game written in es6 javascript with testing",
  "main": "src/index.js",
  "scripts": {
    "prebuild": "rimraf docs",
    "build": "webpack -p --env.prod",
    "dev": "webpack-dev-server --env.dev",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "jest": {
    "collectCoverageFrom": [
      "*.js"
    ],
    "coverageThreshold": {
      "global": {
        "statements": 50,
        "branches": 50,
        "functions": 50,
        "lines": 100
      }
    }
  },
  "author": "steve barton <bevets87@gmail.com>",
  "license": "ISC",
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2016": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "css-loader": "^0.28.4",
    "eslint": "^4.4.0",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "jest": "^20.0.4",
    "lodash": "^4.17.4",
    "node-sass": "^4.5.3",
    "npm-run-all": "^4.0.2",
    "rimraf": "^2.6.1",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.18.2",
    "webpack": "^3.4.1",
    "webpack-dev-server": "^2.6.1"
  }
}
